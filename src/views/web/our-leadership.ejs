<div class="banner">
	<div class="carousel fade-carousel slide" data-ride="carousel" data-interval="4000" id="bs-carousel">
		<div class="overlay"></div>
		<ol class="carousel-indicators">
			
			<%for (var i=0; i < leadershipbanner.length; i++) {%>
				<li data-target="#bs-carousel" data-slide-to="0" class="<%=i==0?"active":""%>"></li>
		  <%}%>
		 </ol>
		 <div class="carousel-inner">

			<%for (var i=0; i < leadershipbanner.length; i++) {%>
		    <div class="item slides active">
		      	<img src="<%=leadershipbanner[i].image.replace('public', '/static')%>"/>
		      	<div class="carousel-caption1" data-aos="fade-up" data-aos-duration="1500">
		      		<h2><%=leadershipbanner[i].name%></h2>
		      	</div>
		    </div>
<%}%>
	  	</div> 
	</div>
